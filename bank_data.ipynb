    "dataset = dataframe.iloc[:, 3].values\n",
    "\n",
    "banks = {}\n",
    "try:\n",
    "    for i in dataset:\n",
    "        url = f'https://www.routingnumbers.info/api/name.json?rn={i}'\n",
    "        response = requests.get(url)\n",
    "        data = json.loads(response.content)\n",
    "        if data['message'] == 'OK':\n",
    "            banks[i] = data['name']\n",
    "        else:\n",
    "            pass\n",
    "except:\n",
    "    pass\n",
    "\n",
    "routing = []\n",
    "bank_name = []\n",
